{
  "name": "outline-zh-comments",
  "displayName": "Outline Chinese Comments",
  "description": "在 VS Code 自带大纲中显示中文注释，支持悬停提示和代码镜头",
  "version": "0.0.1",
  "publisher": "wonvy",
  "author": {
    "name": "Wonvy",
    "email": "wonvy@example.com"
  },
  "icon": "icon-256.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wonvy/outline-zh-comments.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "outline",
    "chinese",
    "comments",
    "documentation",
    "typescript",
    "javascript"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "documentSymbol": [
      {
        "language": "typescript"
      },
      {
        "language": "javascript"
      },
      {
        "language": "typescriptreact"
      },
      {
        "language": "javascriptreact"
      }
    ],
    "hover": [
      {
        "language": "typescript"
      },
      {
        "language": "javascript"
      },
      {
        "language": "typescriptreact"
      },
      {
        "language": "javascriptreact"
      }
    ],
    "codeLens": [
      {
        "language": "typescript"
      },
      {
        "language": "javascript"
      },
      {
        "language": "typescriptreact"
      },
      {
        "language": "javascriptreact"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x"
  }
}